{
  "compilerOptions": {
    "module": "umd",
    "target": "es5",
    "sourceMap": true,
    "strict": true,
    "removeComments": false,
    "noEmitHelpers": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "typings",
    "tests",
    "node_modules",
    "bower_components",
    "dist",
    ".nuget",
    "documentation"
  ]
}
